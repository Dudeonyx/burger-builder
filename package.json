{
  "name": "burger-builder",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dudeonyx.github.io/burger-builder",
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@redux-ts-starter-kit/core": "^2.0.0-alpha.4",
    "axios": "^0.18.0",
    "immer": "^2.0.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.2"
  },
  "scripts": {
    "analyze": "yarn build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "yarn build --stats",
    "deploy": "gh-pages -d build",
    "lint": "tslint --force \"src/**/*.ts?(x)\"",
    "lint_fix": "tslint --fix --force \"src/**/*.ts?(x)\"",
    "typecheck": "tsc",
    "analyzePrevious": "webpack-bundle-analyzer build/bundle-stats.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "space-in-parens": [
        "warn",
        "always"
      ]
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.10",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint-plugin-react-hooks": "^1.0.1",
    "gh-pages": "^2.0.1",
    "prettier-tslint": "^0.4.0",
    "redux-devtools-extension": "^2.13.7",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "tslint-react-hooks": "^1.1.0",
    "typescript": "^3.2.2",
    "typescript-styled-plugin": "^0.13.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
