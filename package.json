{
  "name": "burger-builder",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dudeonyx.github.io/burger-builder/",
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "react": "^16.6.3",
    "react-classer": "^0.1.0",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "serve": "yarn run serve -s build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "space-in-parens": [
        "warn",
        "always"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@storybook/addon-actions": "^4.1.0-alpha.8",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/addon-links": "^4.1.0-alpha.8",
    "@storybook/addons": "^4.1.0-alpha.8",
    "@storybook/react": "^4.1.0-alpha.8",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.10",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__react": "^4.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "gh-pages": "^2.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  },
  "optionalDependencies": {
    "@storybook/cli": "next",
    "serve": "latest"
  }
}
