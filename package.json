{
  "name": "burger-builder",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dudeonyx.github.io/burger-builder",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@redux-ts-starter-kit/core": "^2.0.0-alpha.4",
    "axios": "^0.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "analyze": "yarn build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "yarn build --stats",
    "deploy": "gh-pages -d build",
    "lint": "tslint --force \"src/**/*.ts?(x)\"",
    "lint_fix": "tslint --fix --force \"src/**/*.ts?(x)\"",
    "typecheck": "tsc",
    "analyzePrevious": "webpack-bundle-analyzer build/bundle-stats.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "^2.0.1",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "^3.4.1",
    "typescript-styled-plugin": "^0.14.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
